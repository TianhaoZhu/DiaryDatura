<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/lineHeight" android:color="@color/line_color" />
            <solid android:color="@color/workSpaceBtnNormal" />
            <corners android:radius="@dimen/padding_four" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/white" />
            <stroke android:width="@dimen/lineHeight" android:color="@color/line_color" />
            <corners android:radius="@dimen/padding_four" />
        </shape>
    </item>
</selector>